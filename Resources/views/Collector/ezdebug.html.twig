{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {%  set data = collector.getData() %}

    <div class="sf-toolbar-block">
        <div class="sf-toolbar-icon">
            <a href="#">
                <img alt="NM" src="data:image/png;base64," />
            </a>
        </div>

        <div class="sf-toolbar-info">

            {% if data.version %}
                <div class="sf-toolbar-info-piece">
                    <b>eZPublish</b>
                    <span class="sf-toolbar-status sf-toolbar-status-green">{{ data.version }}</span>
                </div>

                <div class="sf-toolbar-info-piece">
                    <a href="https://doc.ez.no" rel="help">eZPublish Documentation</a>
                </div>
            {% endif %}

            {% if data.bundles %}
                <div class="sf-toolbar-info-piece">
                    <b>Bundles</b>
                    <span class="sf-toolbar-status sf-toolbar-status-green">{{ data.bundles|length }}</span>
                </div>
            {% endif %}

        </div>
    </div>
{% endblock %}

{% block head %}
    {# if the web profiler panel needs some specific JS or CSS files #}
{% endblock %}

{% block menu %}
    {# the menu content #}
{% endblock %}

{% block panel %}
    {# the panel content #}
{% endblock %}